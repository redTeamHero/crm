{
  "BALANCE_GT_CL": {
    "violation": "Balance exceeds reported Credit Limit",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "critical",
    "fieldsImpacted": ["balance","credit_limit"],
    "rule": {"all":[{"field":"balance","gt_field":"credit_limit"}]}
  },

  "CURRENT_BUT_PASTDUE": {
    "violation": "Account marked Current but Past Due > 0",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["account_status","past_due"],
    "rule":{"all":[{"field":"account_status","eq":"Current"},{"field":"past_due","gt":0}]}
  },

  "DOLP_BEFORE_OPEN": {
    "violation": "Date of Last Payment occurs before Date Opened",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["date_last_payment","date_opened"],
    "rule":{"all":[{"field":"date_last_payment","lt_field":"date_opened"}]}
  },


  "DOLP_EQUALS_OPEN_DATE": {
    "violation": "Date of Last Payment equals Date Opened",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["date_last_payment","date_opened"],
    "rule":{"all":[{"field":"date_last_payment","eq_field":"date_opened"}]}
  },

  "DOLP_MOVED_FORWARD": {
    "violation": "Re-aging: Date of Last Payment moved forward",
    "target": "tradeline",
    "fcraSection": "§623(a)(5)",
    "severity": "critical",
    "fieldsImpacted": ["date_last_payment","historical_dolp"],
    "rule":{"all":[{"field":"date_last_payment","gt_field":"historical_dolp"}]}
  },

  "CHARGEOFF_DATE_BEFORE_DOLP": {
    "violation": "Charge-off date before Date of Last Payment",
    "target": "collection",
    "fcraSection": "§623(a)(5)",
    "severity": "critical",
    "fieldsImpacted": ["date_chargeoff","date_last_payment"],
    "rule":{"all":[{"field":"date_chargeoff","lt_field":"date_last_payment"}]}
  },

  "CHARGEOFF_PASTDUE_PRESENT": {
    "violation": "Charge-off account still shows Past Due amount",
    "target": "collection",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["account_status","past_due"],
    "rule":{"all":[{"field":"account_status","eq":"Charge-off"},{"field":"past_due","gt":0}]}
  },

  "CHARGEOFF_WITH_BALANCE": {
    "violation": "Balance still changing after charge-off",
    "target": "collection",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["balance","account_status"],
    "rule":{"all":[{"field":"account_status","eq":"Charge-off"},{"field":"balance","gt":0}]}
  },

  "HIGH_CREDIT_LT_BALANCE": {
    "violation": "High Credit less than Balance",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["high_credit","balance"],
    "rule":{"all":[{"field":"high_credit","lt_field":"balance"}]}
  },

  "INSTALLMENT_BALANCE_GT_ORIGINAL": {
    "violation": "Installment balance greater than original loan amount",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["balance","original_amount"],
    "rule":{"all":[{"field":"balance","gt_field":"original_amount"}]}
  },

  "PASTDUE_GT_BALANCE": {
    "violation": "Past Due exceeds Balance",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["past_due","balance"],
    "rule":{"all":[{"field":"past_due","gt_field":"balance"}]}
  },

  "PASTDUE_WITH_ZERO_BALANCE": {
    "violation": "Past Due > 0 while Balance = 0",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["past_due","balance"],
    "rule":{"all":[{"field":"past_due","gt":0},{"field":"balance","eq":0}]}
  },

  "NEGATIVE_BALANCE": {
    "violation": "Negative balance reported",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["balance"],
    "rule":{"all":[{"field":"balance","lt":0}]}
  },






  "UPDATE_BEFORE_OPEN": {
    "violation": "Reported date before open date",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["last_reported","date_opened"],
    "rule":{"all":[{"field":"last_reported","lt_field":"date_opened"}]}
  },

  "REPORT_DATE_TOO_OLD_ACTIVE": {
    "violation": "Active account not updated in >1 months",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["last_reported","account_status"],
    "rule":{"all":[{"field":"account_status","neq":"Closed"},{"field":"last_reported","lt":"1M"}]}
  },

  "MISSING_DATE_REPORTED": {
    "violation": "Missing Date Reported on derogatory account",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["last_reported"],
    "rule":{"all":[{"field":"last_reported","exists":false}]}
  },

  "MISSING_DOLP": {
    "violation": "Missing Date of Last Payment on derogatory account",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["date_last_payment",""],
    "rule":{"all":[{"field":"date_last_payment","exists":false}]}
  },

  "PAYMENT_HISTORY_AFTER_CLOSE": {
    "violation": "Payment history after closure date",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["payment_history","date_closed"],
    "rule":{"all":[{"field":"payment_history_date","gt_field":"date_closed"}]}
  },

  "LIMIT_MISSING_ON_REVOLVING": {
    "violation": "No limit on revolving account",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["credit_limit","account_type"],
    "rule":{"all":[{"field":"account_type","eq":"Revolving"},{"field":"credit_limit","exists":false}]}
  },

  "OPEN_ACCOUNT_ZERO_LIMIT": {
    "violation": "Open revolving account with 0 limit",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["account_status","credit_limit"],
    "rule":{"all":[{"field":"account_status","eq":"Open"},{"field":"credit_limit","eq":0}]}
  },

  "OPEN_ACCOUNT_ZERO_PAYMENT": {
    "violation": "Open installment account with 0 payment",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "moderate",
    "fieldsImpacted": ["account_status","monthly_payment"],
    "rule":{"all":[{"field":"account_status","eq":"Open"},{"field":"monthly_payment","eq":0}]}
  },

  "TERMS_MISSING_ON_MORTGAGE": {
    "violation": "No term length on mortgage",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["account_type","no_of_months_terms"],
    "rule":{"all":[{"field":"account_type","contains":"Mortgage"},{"field":"no_of_months_terms","exists":false}]}
  },

  "REPORTING_AFTER_ZERO_BAL_CLOSED": {
    "violation": "Closed zero-balance account still reporting monthly",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["balance","account_status","last_reported"],
    "rule":{"all":[{"field":"balance","eq":0},{"field":"account_status","eq":"Closed"},{"field":"last_reported","gt":"90D"}]}
  },

  "UTILIZATION_SPIKE_XB": {
    "violation": "Utilization spike = 50% during dispute period",
    "target": "cross_bureau",
    "fcraSection": "§611(a)",
    "severity": "moderate",
    "fieldsImpacted": ["credit_limit","balance","compliance_code"],
    "rule":{"all":[{"field":"compliance_code","in":["XB","XF"]},{"expr":"(balance/credit_limit)","gt":0.5}]}
  },

  "ZERO_BAL_WITH_PAYMENT_DUE": {
    "violation": "Zero balance but scheduled payment present",
    "target": "tradeline",
    "fcraSection": "§607(b)",
    "severity": "minor",
    "fieldsImpacted": ["balance","scheduled_payment_amount"],
    "rule":{"all":[{"field":"balance","eq":0},{"field":"scheduled_payment_amount","gt":0}]}
  }
}
